<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Inventory App</title>
  </head>
  <body>
    <h1>Databases list:</h1>
    <ul>
      <% dbList.forEach((db) => { %>
      <li>
        <button>
          <a href="/<%= db.table_name %>">
            <%= db.table_name.charAt(0).toUpperCase() + db.table_name.slice(1)
            %></a
          >
        </button>
      </li>
      <% }); %>
    </ul>
  </body>
</html>
